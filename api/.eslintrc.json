{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "max-len": ["error", { "code": 140, "tabWidth": 4 }],
    "new-cap": ["off"]
  }
}
